<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_mentorhomev2"

        tools:context=".Mentorhomev2" android:background="@color/about_libraries_special_button_openSource_dark">
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/wants_to_create_new_slots_n_click_on_start_new_session"
            android:fontFamily="@font/mate_sc"
            android:textColor="@color/about_libraries_dividerDark_openSource_dark"
            android:textSize="17dp"
            android:id="@+id/textView9"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.497" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="128dp"
            android:textAlignment="center"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/existing_session_value"
            android:fontFamily="@font/mate_sc"
            android:textColor="@color/about_libraries_dividerDark_openSource_dark"
            android:textSize="17dp"
            android:id="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.493" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider5" android:layout_marginTop="100dp"
            android:textAlignment="center"/>
    <Button
            android:text="Start New Session"
            android:layout_width="250dp"
            android:background="@color/md_red_A700"
            android:fontFamily="@font/kurale"
            android:layout_height="wrap_content"
            android:id="@+id/new_session_btn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.496" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/divider5" android:layout_marginBottom="104dp"
            android:textAllCaps="false" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintVertical_bias="0.111"/>
    <Button
            android:text="Manage Existing Session"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:background="@color/md_red_A700"
            android:fontFamily="@font/kurale"
            android:id="@+id/existing_session_btn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.496" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="108dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView7"
            android:textAllCaps="false" app:layout_constraintVertical_bias="0.057"/>
    <View
            android:id="@+id/divider5"
            android:layout_width="395dp"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider"
            tools:layout_editor_absoluteX="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" tools:ignore="MissingConstraints"/>
</androidx.constraintlayout.widget.ConstraintLayout>