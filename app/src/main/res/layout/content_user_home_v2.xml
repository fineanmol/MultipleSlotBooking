<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_user_home_v2"
        tools:context=".UserHomeV2"
        android:background="@color/about_libraries_special_button_openSource_dark">
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:fontFamily="@font/rosemary"
            android:textColor="@color/about_libraries_dividerDark_openSource_dark"
            android:textSize="17dp"
            android:id="@+id/textviewstatus"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.513"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/newooking"
            android:textColor="@color/about_libraries_dividerDark_openSource_dark"
            android:textSize="17dp"
            android:id="@+id/textView9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="84dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.513" android:fontFamily="@font/mate_sc"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/already_booked_show"
            android:textColor="@color/about_libraries_dividerDark_openSource_dark"
            android:textSize="17dp"
            android:id="@+id/textView7"
            android:fontFamily="@font/mate_sc"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.502" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/new_book_btn" app:layout_constraintVertical_bias="0.341"
            android:layout_marginStart="8dp"/>


    <Button android:layout_width="210dp" android:layout_height="wrap_content"
            android:text="Show your Appointment"
            android:id="@+id/show_appointment_btn"
            app:layout_editor_absoluteY="20dp"
            app:layout_editor_absoluteX="60dp"
            android:fontFamily="@font/kurale"
            android:background="@color/md_red_A700"
            app:layout_constraintEnd_toEndOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/textView7"
            android:layout_marginTop="56dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7" app:layout_constraintHorizontal_bias="0.357"
            android:layout_marginStart="8dp"/>
    <Button android:layout_width="140dp"
            android:text="Book Now"

            android:background="@color/md_red_A700"
            android:id="@+id/new_book_btn"
            android:layout_height="wrap_content"
            android:fontFamily="@font/kurale"
            app:layout_constraintEnd_toEndOf="@+id/textView9" app:layout_constraintStart_toStartOf="@+id/textView9"
            android:layout_marginTop="48dp" app:layout_constraintTop_toBottomOf="@+id/textView9"
            app:layout_constraintHorizontal_bias="0.498"/>
</androidx.constraintlayout.widget.ConstraintLayout>
